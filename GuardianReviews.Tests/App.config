<?xml version="1.0" encoding="utf-8" ?>
<!-- 
    Note: Add entries to the App.config file for configuration settings
    that apply only to the Test project.
-->
<configuration>
    <appSettings>
        <add key="DataConnectionString" value="UseDevelopmentStorage=true" />
        <add key="ApiUrl" value="http://content.guardianapis.com/"/>
        <add key="ApiKey" value="techdev-approved"/>
        <add key="ApiPageSize" value="10"/>
		<!-- Accepts a comma delimited list of assembly names containing mapping artifacts; the ".dll" is optional -->
		<add key="nhibernate.mapping.assembly" value="GuardianReviews.NHibernate" />
    </appSettings>

    <connectionStrings>
        <add name="GuardianReviews.ConnectionString" connectionString="Data Source=.;Initial Catalog=GuardianReviews;Integrated Security=True" providerName="System.Data.EntityClient" />
    </connectionStrings>
</configuration>
